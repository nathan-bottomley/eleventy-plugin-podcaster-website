<h1>Podcaster Blog</h1>

{% for post in collections.post | reverse %}
<article>
  <h2><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
  <p class="post-date"><time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time></p>
  {{ post.content | safe }}
</article>
{% endfor %}